include "csv.nhp"
include "std.nhp"
include "mem.nhp"

def main():
	def testfun():
		myTable = new ndarray<int>([12, 12], 0)
		std::repeat(12, lambda int i: std::repeat(12, lambda int j: myTable.set([i, j], i * j)))
		csv::save("timesTable.csv", myTable, lambda io::writer writer, int i: writer.write(i))
	mem::leaksan(testfun)