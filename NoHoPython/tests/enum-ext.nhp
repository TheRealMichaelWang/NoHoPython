include "io.nhp"

enum dayOfWeek:
	Monday
	Tuesday
	Wednessday
	Thursday
	Friday
	Saturday
	Sunday

enum result<T>:
	T
	error

def getDayOfWeek(int dayNo) result<dayOfWeek>:
	if dayNo < 0 or dayNo >= 7:
		return result::error
	days = [<dayOfWeek> dayOfWeek::Monday, dayOfWeek::Tuesday, dayOfWeek::Wednessday, dayOfWeek::Thursday, dayOfWeek::Friday, dayOfWeek::Saturday, dayOfWeek::Sunday]
	return days[dayNo]

def main():
	print("Day?>")
	dayNo = input() as int

	day = getDayOfWeek(dayNo) as dayOfWeek
	match day:
		dayOfWeek::Sunday:
			print("sun")
		dayOfWeek::Saturday:
			print("Sat")
		default:
			print("Fuck")