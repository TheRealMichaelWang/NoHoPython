include "stdlib/io.nhp"

class student:
	array<char> name
	int age
	int grade
	
	def __init__(array<char> name, int age, int grade):
		self.name = name
		self.age = age
		self.grade = grade
	def toString() array<char>:
		return self.name

class faculty:
	array<char> name
	int age
	int payment
	
	def __init__(array<char> name, int age, int payment):
		self.name = name
		self.age = age
		self.payment = payment
	def toString() array<char>:
		return self.name

interface person:
	array<char> name
	int age
	
	fn<array<char>> toString

enum schoolperson: printableObj:
	student
	faculty
	int
	char

def printSchoolPerson(schoolperson sp):
	match sp:
		student s:
			io::print("student: ")
		faculty f:
			io::print("faculty: ")
		default:
			io::print("dontfuckingknow: ")
	#io::println(sp)

def main():
	printSchoolPerson(new student("michael", 17, 11))
	printSchoolPerson(new faculty("maine", 40, 140))
	printSchoolPerson(100)