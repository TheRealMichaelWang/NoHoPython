class student:
	string name
	int age
	int grade
	
	pure __init__(string name, int age, int grade):
		self.name = name
		self.age = age
		self.grade = grade
	
	pure to_string() string:
		return self.name

class faculty:
	string name
	int age
	int payment
	
	pure __init__(string name, int age, int payment):
		self.name = name
		self.age = age
		self.payment = payment

	pure to_string() string:
		return self.name

interface person:
	string name
	int age
	pure<string> to_string

enum schoolperson:
	student
	faculty
	int
	char

def enumMatchTest(schoolperson p):
	match p:
		student:
			p.grade = 11
		faculty:
			p.name = "Random Faculty Name"
		default:
			return

def printPersonInfo<T : person>(T p):
	print($"Name: {p.name}");
	print($"Age: {p.age}")

def main():
	michael = new student("michael", 17, 11)
	maine = new faculty("maine", 40, 140)
	printPersonInfo(michael)
	print(maine)
	enumMatchTest(michael)
	enumMatchTest(maine)
	printPersonInfo(michael)
	print(maine)