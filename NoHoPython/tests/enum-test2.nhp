include "io.nhp"

class student:
	string name
	int age
	int grade
	
	def __init__(string name, int age, int grade):
		self.name = name
		self.age = age
		self.grade = grade
	
	def to_string() string:
		return self.name

class faculty:
	string name
	int age
	int payment
	
	def __init__(string name, int age, int payment):
		self.name = name
		self.age = age
		self.payment = payment

	def to_string() string:
		return self.name

interface person:
	string name
	int age
	
	fn<string> to_string

enum schoolperson:
	student
	faculty
	int
	char

def main():
	print(new student("michael", 17, 11))
	print(new faculty("maine", 40, 140))