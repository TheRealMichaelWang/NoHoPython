interface person:
	string name
	int age

class student:
	string name
	int age
	int grade

	def __init__(string name, int age, int grade):
		self.name = name
		self.age = age
		self.grade = grade

def comparePerson(person p, person s):
	assert p.name == s.name
	assert p.age == s.age

def main():
	michael = new student("Michael", 17, 11)
	comparePerson(new student("tim", 18, 12), michael)