interface person:
	string name
	int age

class student:
	string name
	int age
	int grade

	pure __init__(string name, int age, int grade):
		self.name = name
		self.age = age
		self.grade = grade
		
mod typeExt:
	pure person_equals(person a, person b) bool:
		return p.name == s.name and p.age == s.age

def main():
	michael = new student("Michael", 17, 11)
	assert michael == new student("tim", 18, 12)